<html>
<head>
    <title>ProtoDoc for: {{packageName}}.{{fieldName}}</title>
    {{> styles_css}}
    {{> message_styles_css}}
</head>
<body>
<h1 class="message-header">{{messageName}}</h1>

<h2>defined in package: {{packageName}}</h2>

<h2 class="fields-header sub-header">Fields:</h2>
<ul class="fields">
    {{#fields}}
    <li>
        <strong>{{fieldName}}</strong><br/>
        defined as: {{protoTypeName}}<br/>
        mapped to: {{scalaTypeName}}
    </li>
    {{/fields}}
    {{^fields}}
    <li>
        This message defines no fields.
    </li>
    {{/fields}}
</ul>

<h2 class="inner-enums-header sub-header">Inner Enums:</h2>
<ul class="inner-enums">
    {{#enums}}
    <li>
        <strong>{{typeName}}</strong><br/>
        defines values:
        <table>
            <thead>
            <tr>
                <td>Value</td>
                <td>TAG</td>
                <td>Comment</td>
            </tr>
            </thead>
            <tbody>
            {{#values}}
            <tr>
                <td class="code">{{valueName}}</td>
                <td>{{tag}}</td>
                <td>{{comment}}</td>
            </tr>
            {{/values}}
            </tbody>
        </table>
    </li>
    {{/enums}}
    {{^enums}}
    <li>
        This message defines no enums.
    </li>
    {{/enums}}
</ul>

<h2 class="inner-msgs-header sub-header">Inner messages:</h2>
<ul class="inner-msgs">
    {{#innerMessages}}
    <li>
        <strong>
            <a class="msg-link" href="{{packageName}}.{{messageName}}.html">
                {{messageName}}
            </a>
        </strong>
    </li>
    {{/innerMessages}}
    {{^innerMessages}}
    <li>
        This message defines no inner messages.
    </li>
    {{/innerMessages}}
</ul>

</body>
</html>